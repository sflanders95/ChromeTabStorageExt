{
    "name": "myChromeExt",
    "version": "1.0",
    "author": "Steven Flanders",
    "description": "Save Current Open Tabs",
    "permissions": ["tabs", "activeTab", "declarativeContent", "storage", "*://*/* "],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "html/main.html"
    },
    "options_page": "html/options.html",
    "orther_page": "html/options.html",
    "manifest_version": 2
}
